hook OnPlayerKeyStateChange(playerid, newkeys, oldkeys) 
{
    if (newkeys & KEY_SECONDARY_ATTACK) {
        if (IsPlayerInRangeOfPoint(playerid, 2.0, POO_ROOF_POINT)) { // Roof Enter
            if (Spieler[playerid][pFraktion] != 5) return SendClientMessage(playerid, COLOR_RED, "Du besitzt keine gültige Schlüsselkarte für das Oamt-Gebäude.");
            SetPlayerFacingAngle(playerid, POO_INTERIOR_ROOF_POINT_FACING);
            SetCameraBehindPlayer(playerid);
            SetPlayerPosEx(playerid, POO_INTERIOR_ROOF_POINT, MAPS_POOINTERIOR_INTERIOR, VW_POOINTERIOR);
        }
    }
    return 1;
}

